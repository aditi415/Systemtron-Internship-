studentsdb=# SELECT * FROM Students ORDER BY date_of_birth DESC;

studentsdb=# select s.first_name, s.last_name, c.course_name FROM Enrollments e JOIN Students s ON e.student_id = s.student_id JOIN Courses c ON e.course_id = c.course_id;
 first_name | last_name |    course_name     
------------+-----------+--------------------
